{
  "build": {
    "devPath": "http://sucicada.cf",
    "withGlobalTauri": false,
    "distDir": "."
  },
  "package": {
    "productName": "Lain-voice",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.sucicada.lain_voice",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "width": 800,
        "height": 600,
        "title": "Lain Voice"
      }
    ]
  }
}
